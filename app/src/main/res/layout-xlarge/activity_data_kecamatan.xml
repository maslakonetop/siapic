<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".DataKecamatan">

    <include
        layout="@layout/menu_atas"
        android:layout_width="111dp"
        android:layout_height="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="258dp"
        android:layout_height="31dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="100dp"
        app:layout_constraintBottom_toTopOf="@+id/map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        app:srcCompat="@drawable/apik"
        tools:ignore="ContentDescription" />

    <Spinner
        android:id="@+id/spKecamatan"
        android:layout_width="@dimen/_157sdp"
        android:layout_height="@dimen/_27sdp"
        android:layout_marginStart="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/txtPilihKecamatan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtPilihKecamatan"
        app:layout_constraintTop_toTopOf="@+id/txtPilihKecamatan"
        app:layout_constraintVertical_bias="1.0" />

    <Spinner
        android:id="@+id/spPotensi"
        android:layout_width="@dimen/_157sdp"
        android:layout_height="@dimen/_27sdp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/spKecamatan"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/spKecamatan"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.23" />

    <TextView
        android:id="@+id/txtPilihKecamatan"
        android:layout_width="209dp"
        android:layout_height="47dp"
        android:fontFamily="@font/arial"
        android:text="Pilih Kecamatan"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.187"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="219dp"
        android:layout_height="39dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/arial"
        android:text="Pilih Potensi"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/map"
        app:layout_constraintEnd_toEndOf="@+id/txtPilihKecamatan"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/txtPilihKecamatan"
        app:layout_constraintTop_toBottomOf="@+id/txtPilihKecamatan"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="777dp"
        android:layout_height="846dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.238"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:context=".MapDataKecamatan" />

    <Button
        android:id="@+id/btnPeta"
        android:layout_width="206dp"
        android:layout_height="47dp"
        android:background="@color/goldenrot"
        android:text="Tampilkan Peta"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/map"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.304"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnPdf"
        android:layout_width="172dp"
        android:layout_height="52dp"
        android:background="@color/goldenrot"
        android:text="Data Detail"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/btnPeta"
        app:layout_constraintEnd_toEndOf="@+id/spPotensi"
        app:layout_constraintHorizontal_bias="0.959"
        app:layout_constraintStart_toEndOf="@+id/btnPeta"
        app:layout_constraintTop_toTopOf="@+id/btnPeta"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="514dp"
        android:layout_marginRight="25dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="utama"
        app:backgroundTint="@color/aqua"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.927"
        app:srcCompat="@drawable/back" />
</androidx.constraintlayout.widget.ConstraintLayout>